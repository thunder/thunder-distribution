interface Media {
  id: Int!
  uuid: String!
  entity: String!
  name: String
}

type MediaImage implements Media {
  id: Int!
  uuid: String!
  entity: String!
  name: String

  title: String
  alt: String
  copyright: String
  description: String
  src: String
  width: Int
  height: Int
  tags: [Tag]
}

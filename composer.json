    "extra": {
        "installer-name": "thunder",
        "patches": {
            "drupal/core": {
                "Fix Claro styles for exposed views filters wrapped in fieldsets": "https://www.drupal.org/files/issues/2020-07-30/3133639-8.patch",
                "It is possible to overflow the number of items allowed in Media Library": "https://git.drupalcode.org/project/drupal/-/merge_requests/760.diff",
                "Let users configure the text of the \"Add media\" button": "https://www.drupal.org/files/issues/2021-10-01/3169956-34.patch"
            },
            "drupal/diff": {
                "Back button for comparison page": "https://www.drupal.org/files/issues/back_button_for-2853193-4.patch"
            },
            "drupal/password_policy": {
                "Policies should declare a config dependency on the plugin modules": "https://www.drupal.org/files/issues/2020-04-14/2918974-7-11.patch"
            },
            "drupal/field_group": {
                "Undefined property: stdClass::$region in field_group_form_process()": "https://www.drupal.org/files/issues/2020-06-15/3059614-37.patch"
            },
            "drupal/dropzonejs": {
                "Make dropzonejs widget available to the new Media Library in core": "https://www.drupal.org/files/issues/2021-06-25/3051658-35.patch"
            },
            "drupal/simple_sitemap": {
                "Sitemap variant index functionality": "https://www.drupal.org/files/issues/2021-03-03/simple_sitemap-sitemap_variant_index-3109090-16.patch"
            },
            "drupal/gin": {
                "Add a form description toggle": "https://git.drupalcode.org/project/gin/-/merge_requests/12.diff",
                "Collapsible sidebar": "https://git.drupalcode.org/project/gin/-/merge_requests/13.diff",
                "Move all action buttons into gin_actions / module incompatibility": "https://www.drupal.org/files/issues/2021-09-06/move_all_actions-3219020-5.patch"
            },
            "drupal/media_library_media_modify": {
                "Override modal shows 'Remove' button for image": "https://git.drupalcode.org/project/media_library_media_modify/-/merge_requests/4.diff"
            },
            "drupal/entity_reference_actions": {
                "Allow subclasses of EntityReferenceItem ": "https://git.drupalcode.org/project/entity_reference_actions/-/merge_requests/1.diff"
            }
        }
    },

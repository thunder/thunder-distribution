{
    "name": "thunder/thunder-distribution",
    "description": "The thunder distribution",
    "type": "drupal-profile",
    "authors": [
        {
            "name": "Daniel Bosen",
            "email": "daniel.bosen@burda.com"
        },
        {
            "name": "Christian Fritsch",
            "email": "christian.fritsch@burda.com"
        },
        {
            "name": "Mladen Todorovic",
            "email": "mladen.todorovic@burda.com"
        },
        {
            "name": "Volker Killesreiter",
            "email": "volker.killesreiter@burda.com"
        },
        {
            "name": "Alex Pott",
            "email": "alex.a.pott@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "installer-name": "thunder",
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/diff": {
                "Back button for comparison page": "https://www.drupal.org/files/issues/back_button_for-2853193-4.patch"
            },
            "drupal/amp": {
                "Missing schema": "https://www.drupal.org/files/issues/missing_schema_for-2878769-3.patch",
                "Remove deprecated code": "https://www.drupal.org/files/issues/2020-01-10/3078897-9.patch"
            },
            "drupal/amptheme": {
                "Property 'base theme' needs to be set": "https://www.drupal.org/files/issues/2020-01-14/3106310.patch"
            },
            "drupal/entity_browser": {
                "Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2019-07-04/2858438_17.patch",
                "Re-order + remove broken with the Entity Reference (and File) widget": "https://www.drupal.org/files/issues/2019-10-18/entity-browser-reorder-remove-2851580-67.patch"
            },
            "drupal/google_analytics": {
                "Drupal 9 compatibility related issues should be fixed": "https://www.drupal.org/files/issues/2020-03-27/3072207-38.patch"
            },
            "drupal/password_policy": {
                "Mark as compatible with Drupal 9": "https://www.drupal.org/files/issues/2020-04-01/3120746-9.patch",
                "Policies should declare a config dependency on the plugin modules": "https://www.drupal.org/files/issues/2019-06-13/2918974-7.patch",
                "Importing configuration on site without password_policy activated": "https://www.drupal.org/files/issues/2020-03-20/password-policy-2771129-110.patch"
            },
            "drupal/config_update": {
                "Make sure Config Update Manager is Drupal 9 compatible": "https://www.drupal.org/files/issues/2020-04-01/3089420-6.patch"
            },
            "drupal/redirect": {
                "Update view to met the Drupal 8.8 views schema": "https://www.drupal.org/files/issues/2019-12-23/3089389-6.patch"
            },
            "drupal/config_profile": {
                "Add core_version_requirement for D9 compatibility": "https://www.drupal.org/files/issues/2020-04-01/3124018-1.patch"
            },
            "drupal/thunder_fia": {
                "Add core_version_requirement for D9 compatibility": "https://www.drupal.org/files/issues/2020-04-01/3124067-1.x.patch"
            },
            "drupal/responsive_preview": {
                "Drupal 9 compatibility": "https://www.drupal.org/files/issues/2020-04-01/3105042-7.patch"
            },
            "drupal/media_entity_pinterest": {
                "Deprecated Code Report": "https://www.drupal.org/files/issues/2020-04-01/3074751-11.patch"
            }
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/amp": "1.x-dev",
        "drupal/amptheme": "1.x-dev",
        "marc1706/fast-image-size": "^1.1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "drupal/access_unpublished": "^1.0-alpha8",
        "drupal/adsense": "^1.2-rc1",
        "drupal/admin_toolbar": "^2.0",
        "drupal/autofill": "^1.0",
        "drupal/autosave_form": "^1.0",
        "drupal/better_normalizers": "^1.0-beta3",
        "drupal/blazy": "^2.0",
        "drupal/checklistapi": "1.x-dev",
        "drupal/core": "8.9.x-dev",
        "drupal/config_selector": "^2.0",
        "drupal/config_update": "1.x-dev",
        "drupal/content_lock": "^2.0",
        "drupal/crop": "^2.0-rc1",
        "drupal/ctools": "^3.1",
        "drupal/default_content": "1.x-dev",
        "drupal/diff": "1.0",
        "drupal/dropzonejs": "^2.0-alpha2",
        "drupal/empty_fields": "^1.0-alpha1",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "2.4",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/field_group": "^3.0-rc2",
        "drupal/focal_point": "^1.2",
        "drupal/google_analytics": "2.x-dev",
        "drupal/inline_entity_form": "^1.0",
        "drupal/ivw_integration": "^2.0",
        "drupal/length_indicator": "^1.0-rc2",
        "drupal/linkit": "5.x-dev",
        "drupal/liveblog": "1.x-dev",
        "drupal/media_entity_instagram": "2.x-dev",
        "drupal/media_entity_pinterest": "2.x-dev",
        "drupal/media_entity_slideshow": "^2.0-alpha1",
        "drupal/media_entity_twitter": "^2.0-alpha1",
        "drupal/media_expire": "^2.2",
        "drupal/metatag": "1.x-dev",
        "drupal/paragraphs": "1.x-dev",
        "drupal/paragraphs_features": "^1.8",
        "drupal/password_policy": "3.x-dev",
        "drupal/pathauto": "1.x-dev",
        "drupal/responsive_preview": "1.x-dev",
        "drupal/redirect": "1.x-dev",
        "drupal/scheduler": "1.x-dev",
        "drupal/scheduler_content_moderation_integration": "^1.1",
        "drupal/select2": "^1.3",
        "drupal/simple_sitemap": "^3.3",
        "drupal/simple_gmap": "^2.0",
        "drupal/shariff": "^1.5",
        "drupal/slick": "^2.0-rc3",
        "drupal/slick_media": "^2.0-alpha2",
        "drupal/thunder_admin": "^3.0",
        "drupal/thunder_fia": "1.x-dev",
        "drupal/token": "1.x-dev",
        "drupal/update_helper": "^1.3",
        "npm-asset/dropzone": "^5.5.1",
        "npm-asset/exif-js": "^2.3.0",
        "npm-asset/blazy": "^1.8",
        "npm-asset/slick-carousel": "^1.8",
        "npm-asset/shariff": "^3.1.1",
        "npm-asset/select2": "^4.0.7"
    },
    "suggest": {
        "drupal/riddle_marketplace": "Riddle marketplace integration",
        "drupal/nexx_integration": "NEXX Video CMS integration",
        "valiton/harbourmaster": "Harbourmaster Single Sign On"
    },
    "replace": {
        "bower-asset/jquery": "*",
        "bower-asset/jqueryui": "*",
        "bower-asset/backbone": "*",
        "bower-asset/underscore": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jqueryui": "*",
        "npm-asset/backbone": "*",
        "npm-asset/underscore": "*"
    },
    "require-dev": {
        "burdamagazinorg/thunder-dev-tools": "dev-master",
        "thunder/testsite_builder": "dev-master",
        "thunder/thunder_performance_measurement": "dev-master",
        "thunder/thunder_testing_demo": "3.x-dev",
        "drupal/config_profile": "1.x-dev",
        "mglaman/phpstan-drupal": "~0.12.0",
        "phpstan/phpstan-deprecation-rules": "~0.12.0"
    },
    "conflict": {
        "masterminds/html5": "^2.4.0"
    }
}

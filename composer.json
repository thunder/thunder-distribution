        "drupal/simple_sitemap": "^4.0",
        "drupal/slick": "^2.6",
        "drupal/token": "^1.7",
        "drupal/update_helper": "^2.0||^3.0.3",
        "drupal/search_api": "^1.17",
        "drupal/views_bulk_edit": "^2.5",

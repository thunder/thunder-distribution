        "drupal/core": "~9.4.0@STABLE",
        "drupal/config_selector": "^2.1",
        "drupal/config_update": "^1.7",
        "drupal/content_lock": "^2.1",
        "drupal/crop": "^2.2",
        "drupal/ctools": "^3.6",
        "drupal/default_content": "^1.0-alpha7||^2.0-alpha2",
        "drupal/diff": "1.1",
        "drupal/dropzonejs": "^2.6",
        "drupal/empty_fields": "^1.0-alpha1",
        "drupal/entity_reference_actions": "^1.0-beta10",

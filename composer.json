{
    "name": "thunder/thunder-distribution",
    "description": "The thunder distribution",
    "type": "drupal-profile",
    "authors": [
        {
            "name": "Daniel Bosen",
            "email": "daniel.bosen@burda.com"
        },
        {
            "name": "Christian Fritsch",
            "email": "christian.fritsch@burda.com"
        },
        {
            "name": "Mladen Todorovic",
            "email": "mladen.todorovic@burda.com"
        },
        {
            "name": "Volker Killesreiter",
            "email": "volker.killesreiter@burda.com"
        },
        {
            "name": "Alex Pott",
            "email": "alex.a.pott@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "installer-name": "thunder",
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/diff": {
                "Back button for comparison page": "https://www.drupal.org/files/issues/back_button_for-2853193-4.patch"
            },
            "drupal/amp": {
                "Missing schema": "https://www.drupal.org/files/issues/missing_schema_for-2878769-3.patch",
                "Remove deprecated code": "https://www.drupal.org/files/issues/2020-01-10/3078897-9.patch"
            },
            "drupal/entity_browser": {
                "Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2019-07-04/2858438_17.patch",
                "Re-order + remove broken with the Entity Reference (and File) widget": "https://www.drupal.org/files/issues/2019-12-20/2851580-72.patch"
            },
            "drupal/default_content": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2019-11-07/default_content-deprecation-3092985-2-8.x-1.x.patch",
                "Drupal 9 Deprecated Code Report 2": "https://www.drupal.org/files/issues/2019-12-02/3098095.patch"
            },
            "drupal/dropzonejs": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2019-11-25/3095062-4.patch"
            },
            "drupal/google_analytics": {
                "Drupal 9 compatibility related issues should be fixed": "https://www.drupal.org/files/issues/2020-01-03/3072207-26.patch"
            },
            "drupal/password_policy": {
                "Remove deprecated code": "https://www.drupal.org/files/issues/2020-01-10/3100148-10.patch"
            },
            "drupal/config_update": {
                "Refactor to not support Drush 8 any more": "https://www.drupal.org/files/issues/2019-11-25/3042621-8.patch"
            },
            "drupal/redirect": {
                "Update view to met the Drupal 8.8 views schema": "https://www.drupal.org/files/issues/2019-12-23/3089389-6.patch",
                "Remove Drupal 8.8 deprecations, compatibility with Drupal 9": "https://www.drupal.org/files/issues/2020-01-04/redirect-8.8-3102143-3.patch"
            },
            "drupal/pathauto": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2019-12-26/3042582-32.patch"
            },
            "drupal/scheduler": {
                "Add new 'core_version_requirement' key to .info.yml": "https://www.drupal.org/files/issues/2020-01-10/3079913-5.patch"
            },
            "drupal/media_entity_twitter": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2020-01-10/3097669-9.patch"
            },
            "drupal/media_entity_instagram": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2020-01-10/3097666-20.patch"
            },
            "drupal/metatag": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2020-01-17/metatag-3042590-44.patch"
            },
            "drupal/token": {
                "Drupal 9 Deprecated Code Report": "https://www.drupal.org/files/issues/2020-01-11/token_deprecated-3042568-14.patch"
            }
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/amp": "1.x-dev",
        "drupal/amptheme": "^1.1",
        "marc1706/fast-image-size": "^1.1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "drupal/access_unpublished": "^1.0-alpha8",
        "drupal/adsense": "^1.2-rc1",
        "drupal/admin_toolbar": "^2.0",
        "drupal/autofill": "^1.0",
        "drupal/autosave_form": "^1.0",
        "drupal/better_normalizers": "^1.0-beta3",
        "drupal/blazy": "^2.0-rc3",
        "drupal/checklistapi": "1.x-dev",
        "drupal/core": "8.9.x-dev",
        "drupal/config_selector": "^2.0",
        "drupal/config_update": "1.x-dev",
        "drupal/content_lock": "^2.0",
        "drupal/crop": "^2.0-rc1",
        "drupal/ctools": "3.x-dev",
        "drupal/default_content": "1.x-dev",
        "drupal/diff": "1.0",
        "drupal/dropzonejs": "^2.0-alpha2",
        "drupal/empty_fields": "^1.0-alpha1",
        "drupal/entity": "1.x-dev",
        "drupal/entity_browser": "2.4",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/fb_instant_articles": "^1.0",
        "drupal/field_group": "^3.0-rc2",
        "drupal/focal_point": "1.x-dev",
        "drupal/google_analytics": "2.x-dev",
        "drupal/inline_entity_form": "1.x-dev",
        "drupal/ivw_integration": "^2.0",
        "drupal/length_indicator": "^1.0-rc2",
        "drupal/linkit": "5.x-dev",
        "drupal/liveblog": "1.x-dev",
        "drupal/media_entity_instagram": "2.x-dev",
        "drupal/media_entity_pinterest": "2.x-dev",
        "drupal/media_entity_slideshow": "2.x-dev",
        "drupal/media_entity_twitter": "2.x-dev",
        "drupal/media_expire": "^2.2",
        "drupal/metatag": "1.x-dev",
        "drupal/paragraphs": "1.x-dev",
        "drupal/paragraphs_features": "^1.7",
        "drupal/password_policy": "3.x-dev",
        "drupal/pathauto": "1.x-dev",
        "drupal/responsive_preview": "^1.0-alpha7",
        "drupal/redirect": "1.5",
        "drupal/scheduler": "1.x-dev",
        "drupal/scheduler_content_moderation_integration": "1.x-dev",
        "drupal/select2": "^1.3",
        "drupal/simple_sitemap": "^3.3",
        "drupal/simple_gmap": "^2.0",
        "drupal/shariff": "^1.5",
        "drupal/slick": "^2.0-rc3",
        "drupal/slick_media": "^2.0-alpha2",
        "drupal/thunder_admin": "^3.0",
        "drupal/token": "1.x-dev",
        "drupal/update_helper": "^1.0",
        "npm-asset/dropzone": "^5.5.1",
        "npm-asset/exif-js": "^2.3.0",
        "npm-asset/blazy": "^1.8",
        "npm-asset/slick-carousel": "^1.8",
        "npm-asset/shariff": "^3.1.1",
        "npm-asset/select2": "^4.0.7"
    },
    "suggest": {
        "drupal/riddle_marketplace": "Riddle marketplace integration",
        "drupal/nexx_integration": "NEXX Video CMS integration",
        "valiton/harbourmaster": "Harbourmaster Single Sign On"
    },
    "replace": {
        "bower-asset/jquery": "*",
        "bower-asset/jqueryui": "*",
        "bower-asset/backbone": "*",
        "bower-asset/underscore": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jqueryui": "*",
        "npm-asset/backbone": "*",
        "npm-asset/underscore": "*"
    },
    "require-dev": {
        "burdamagazinorg/thunder-dev-tools": "dev-master",
        "thunder/testsite_builder": "dev-master",
        "thunder/thunder_performance_measurement": "dev-master",
        "thunder/thunder_testing_demo": "3.x-dev",
        "drupal/config_profile": "^1.0",
        "mglaman/phpstan-drupal": "~0.12.0",
        "phpstan/phpstan-deprecation-rules": "~0.12.0"
    },
    "conflict": {
        "masterminds/html5": "^2.4.0"
    }
}

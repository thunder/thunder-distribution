interface Paragraph {
  summary: [String]
}

type ParagraphImage implements Paragraph {
  summary: [String]

  image: MediaImage
}

type ParagraphTwitter implements Paragraph {
  summary: [String]

  provider: String
  url: String
}

type ParagraphInstagram implements Paragraph {
  summary: [String]

  provider: String
  url: String
}

type ParagraphPinterest implements Paragraph {
  summary: [String]

  provider: String
  url: String
}

type ParagraphText implements Paragraph {
  summary: [String]

  text: String
}

type ParagraphGallery implements Paragraph {
  summary: [String]

  name: String!
  images: [MediaImage]
}

type ParagraphLink implements Paragraph {
  summary: [String]

  links: [Link]
}

type ParagraphVideo implements Paragraph {
  summary: [String]

  provider: String
  url: String @deprecated(reason: "Use reference instead")
  video: MediaVideo @deprecated(reason: "Use entity instead")
  entity: Video
  reference: String
}

type ParagraphQuote implements Paragraph {
  summary: [String]

  text: String
}
